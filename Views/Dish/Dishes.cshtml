@model List<Dish>;

<div class="card">
    <div class="card-header bg-dark text-light">Dishes</div>
    <div class="card-body">
        <table>
            <thead>
                <th>Name</th>
                <th>Chef</th>
                <th>Tastiness</th>
                <th>Calories</th>
            </thead>
                @foreach (var dish in Model)
                {       
                    <tr>
                        <td>@dish.DishName</td>
                        <td>@dish.DishChef</td>
                        <td>@dish.Tastiness</td>
                        <td>@dish.Calories</td>
                    </tr>
                } 
        </table>
    </div>
</div>